- @omniauth_providers = OmniAuth.strategies

%ul.nav.nav-tabs.nav-stacked
  = render :partial => "devise/menu/registration_items"
  = render :partial => "devise/menu/login_items"
  %br
  %li{ role: 'presentation' }
    %a{ href: '#' } Overview
  %li.dropdown{ role: 'presentation' }
    %a.dropdown-toggle{data: { toggle: 'dropdown' },
                       href: '#',
                       role: 'button',
                       "aria-haspopup" => true,
                       "aria-expanded" => false }
      Add a Service
      %span.caret
    - if @omniauth_providers.any?
      %ul.dropdown-menu
        - @omniauth_providers.each do |strategy|
          %li
            = omniauth_link_for strategy

  %li{ role: 'presentation' }
    = link_to 'My Services', my_services_path

